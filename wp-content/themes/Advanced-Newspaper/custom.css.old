#navcats ul li a,#navcats ul li ul li a,#navcats .select ul li a {	font: 15px/1.1em 'palatino linotype',palatino,'times new roman',times,serif;	}#navcats ul li a {	width: 170px;	text-align: center;	line-height:13px;	}#navcats ul li a:hover,#navcats ul li ul li a:hover,#navcats .select ul li a:hover {	text-decoration: underline;	color: #663366;	}#masthead ul li {	display:block	margin: 3px 0 3px 0;	padding: 4px 10px 0px;	}#masthead ul li a {	color: #777777;	}#masthead ul li#menu-item-26 a {	color: black;	}#masthead ul {	border-left: 4px solid #663366;	border-bottom: 1px solid black;	}#masthead {	background: whitesmoke; 	padding: 0px;	}#ad468x60 {	width: 500px;	height: 125px;	}#header {	background-repeat: no-repeat;	}#navcats ul li a:hover, #navcats .current-cat a, #navcats .current-cat-parent a, #navcats .select a{	color:#663366;	}.midColPostMeta, .titleCatName a, .titleCatName a:hover {	display:none;	}#footerPages {	border-top:1px solid black;	}.post img {	border: 0;	}#SideNavMenu {	font: 15px/1.1em 'palatino linotype',palatino,'times new roman',times,serif;	line-height: 22px;	}#SideNavMenu a {	color:black;	padding-bottom: 5px;	}#SideNavMenu a:hover {	color: #663366;	text-decoration: underline;	}#network-nav { /* Added css for the PEBC links to the right of the ednews logo */	float:right;	width:280px;	height:120px;	margin-right:50px;	background:#dedecd url(http://ednewscolorado.org/wp-content/themes/WpAdvNewspaper/styles/default/bg-network-nav.gif) repeat-x top ;	position: relative;}#survey-btn {	height: 104px;	width: 104px;	position: absolute;	left:230px;	top:4px;	z-index: 500;	display: block;}#survey-btn a {//	background: url(http://ednewscolorado.org/wp-content/themes/WpAdvNewspaper/images/btn-survey.png)left no-repeat;	display: block;	height: 104px;	width: 104px;}#survey-btn a:hover {//	background: url(http://ednewscolorado.org/wp-content/themes/WpAdvNewspaper/images/btn-survey.png)right no-repeat;}